{
  "name": "@services/simple-staking",
  "version": "1.0.9",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "NODE_OPTIONS=--max-old-space-size=8192 pnpm exec vite",
    "watchDeps": "./scripts/watch-deps.sh @services/simple-staking",
    "build": "NODE_OPTIONS=--max-old-space-size=8192 pnpm exec tsc --noEmit -p tsconfig.lib.json && NODE_OPTIONS=--max-old-space-size=8192 pnpm exec vite build",
    "lint": "eslint",
    "preview": "vite preview",
    "build:e2e": "NODE_OPTIONS=--max-old-space-size=8192 pnpm exec vite build --mode test",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "clean": "rm -r node_modules",
    "build-docker": "docker build -t babylonlabs/simple-staking -f docker/Dockerfile .",
    "clean-docker": "docker rmi babylonlabs/simple-staking 2>/dev/null; true",
    "prepare": "husky",
    "sort-imports": "eslint --fix .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test e2e/specs/",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report",
    "visualizer": "pnpm exec vite-bundle-visualizer",
    "gen:api": "pnpm exec swagger-typescript-api generate -p https://staking-api.babylonlabs.io/swagger/doc.json -o ./src/shared/utils/swagger -t ./src/shared/utils/swagger/templates -n api.ts --unwrap-response-data --disable-format --silent"
  },
  "dependencies": {
    "@babylonlabs-io/babylon-proto-ts": "workspace:*",
    "@babylonlabs-io/bsn-registry": "workspace:*",
    "@babylonlabs-io/btc-staking-ts": "2.5.7",
    "@babylonlabs-io/core-ui": "workspace:*",
    "@babylonlabs-io/wallet-connector": "workspace:*",
    "@bitcoin-js/tiny-secp256k1-asmjs": "2.2.4",
    "@cosmjs/proto-signing": "0.37.0",
    "@cosmjs/stargate": "0.37.0",
    "@cosmjs/tendermint-rpc": "0.37.0",
    "@reown/appkit": "1.8.13",
    "@sentry/react": "10.23.0",
    "@tanstack/react-query": "5.90.7",
    "@uidotdev/usehooks": "2.4.1",
    "awilix": "12.0.5",
    "bitcoinjs-lib": "7.0.0",
    "core-js": "3.46.0",
    "cosmjs-types": "^0.10.1",
    "date-fns": "4.1.0",
    "decimal.js-light": "2.5.1",
    "idb": "8.0.3",
    "nanoevents": "9.1.0",
    "next-themes": "0.4.6",
    "qs": "6.14.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-error-boundary": "6.0.0",
    "react-hook-form": "7.66.0",
    "react-icons": "5.5.0",
    "react-infinite-scroll-component": "6.1.0",
    "react-router": "7.9.5",
    "react-toastify": "11.0.5",
    "react-tooltip": "5.30.0",
    "sharp": "0.34.4",
    "tailwind-merge": "3.3.1",
    "url-safe-base64": "1.3.0",
    "usehooks-ts": "^3.1.1",
    "uuid": "13.0.0",
    "viem": "2.38.6",
    "wagmi": "2.19.2",
    "yup": "1.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.56.1",
    "@sentry/vite-plugin": "^4.6.0",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@tanstack/react-query-devtools": "5.90.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.0",
    "@types/qs": "^6.14.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/uuid": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "@vitejs/plugin-react": "^5.1.0",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^30.2.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "ecpair": "^3.0.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^16.2.6",
    "msw": "^2.12.0",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "source-map-loader": "^5.0.0",
    "swagger-typescript-api": "^13.2.16",
    "tailwindcss": "^4.1.16",
    "text-encoding": "^0.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vite": "^7.2.0",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vite-plugin-radar": "^0.10.1",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "overrides": {
    "tr46": "4.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --quiet --fix"
    ],
    "*.{js,jsx,ts,tsx,json,css,scss,md,yml,yaml}": [
      "prettier --write --ignore-unknown"
    ]
  }
}
