# Bitcoin Network (REQUIRED)
# Must be "mainnet" or "signet"
# Use "signet" for devnet/testnet, "mainnet" for production
NEXT_PUBLIC_BTC_NETWORK=signet

# Bitcoin Mempool API (optional, has defaults per network)
NEXT_PUBLIC_MEMPOOL_API=https://mempool.signet.babylonlabs.io

# Ethereum Chain ID (REQUIRED)
# Must be "1" (mainnet) or "11155111" (sepolia)
# Use "11155111" for devnet/testnet, "1" for production
NEXT_PUBLIC_ETH_CHAINID=11155111

# Ethereum RPC URL (optional, has defaults per chain ID)
NEXT_PUBLIC_ETH_RPC_URL=https://ethereum-sepolia-rpc.publicnode.com

# Vault Services
NEXT_PUBLIC_VAULT_PROVIDER_RPC_URL=https://btc-vault-api.vault-devnet.babylonlabs.io
NEXT_PUBLIC_VAULT_API_URL=https://vault-indexer-api.vault-devnet.babylonlabs.io

# Trustless Bitcoin Vault (TBV) Contracts
NEXT_PUBLIC_TBV_BTC_VAULTS_MANAGER=0x7390fD5a5FAd260929F6305d2D17b4EA4494D3a0
NEXT_PUBLIC_TBV_VAULT_CONTROLLER=0x6CA6368d3043E14DeeB2E920094b6074f08DdFD8
NEXT_PUBLIC_TBV_BTC_VAULT=0x12ed36F494B705fef9cE31B184e712B86f0Bfd4e
NEXT_PUBLIC_TBV_MORPHO=0x1b832639E0b87bEf28755F5086831deE9671313f
# Reown (WalletConnect) project ID for wallet integration
NEXT_PUBLIC_REOWN_PROJECT_ID=your-reown-project-id-here
