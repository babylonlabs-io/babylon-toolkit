{
  "name": "@babylonlabs-io/babylon-campaigns",
  "version": "0.0.0-semantic-release",
  "type": "module",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/babylonlabs-io/babylon-toolkit.git"
  },
  "main": "dist/index.cjs.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.es.js"
    },
    "./style.css": "./dist/babylon-campaigns.css"
  },
  "scripts": {
    "dev": "npm run storybook",
    "build": "tsc -b --noEmit tsconfig.lib.json && vite build",
    "format": "prettier . --write",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6007",
    "build-storybook": "storybook build",
    "prepare": "husky",
    "release": "npm run build && changeset publish",
    "watch": "nodemon --watch src --ext ts,tsx,css --exec \"npm run build\""
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tailwind-merge": "2.5.4"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.2",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.39.1",
    "@internal/eslint-config": "workspace:*",
    "@semantic-release/github": "^12.0.1",
    "@semantic-release/npm": "^13.1.1",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-onboarding": "^10.0.5",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react": "^10.0.5",
    "@storybook/react-vite": "^10.0.5",
    "@storybook/test": "^8.6.14",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@vitejs/plugin-react": "^5.1.0",
    "autoprefixer": "^10.4.21",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "semantic-release": "^25.0.1",
    "storybook": "^10.0.5",
    "storybook-dark-mode": "^4.0.2",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3",
    "vite": "^7.2.0",
    "vite-plugin-dts": "^4.5.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --quiet --fix",
      "prettier --write --ignore-unknown"
    ]
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  },
  "dependencies": {
    "@babylonlabs-io/core-ui": "workspace:*",
    "react-icons": "5.5.0"
  }
}
