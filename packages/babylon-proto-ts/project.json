{
  "name": "babylon-proto-ts",
  "projectType": "library",
  "root": "packages/babylon-proto-ts",
  "sourceRoot": "packages/babylon-proto-ts/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run build",
        "cwd": "packages/babylon-proto-ts",
        "parallel": false
      }
    }
  },
  "release": {
    "version": {
      "conventionalCommits": true,
      "tagFormat": "${projectName}@${version}",
      "generatorOptions": {
        "fallbackCurrentVersionResolver": "disk"
      }
    }
  }
}