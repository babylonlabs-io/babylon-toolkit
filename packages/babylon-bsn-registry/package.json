{
  "name": "@babylonlabs-io/bsn-registry",
  "version": "0.0.0-semantic-release",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc -b tsconfig.lib.json",
    "prepare": "husky",
    "format": "prettier --check \"src/**/*.ts\"",
    "format:fix": "prettier --write \"src/**/*.ts\"",
    "validate-images": "node scripts/validate-images.js"
  },
  "files": [
    "dist",
    "src/testnet.json",
    "src/mainnet.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/babylonlabs-io/babylon-toolkit.git"
  },
  "keywords": ["babylon", "bsn", "registry", "bitcoin", "testnet", "mainnet"],
  "author": "Babylon Labs",
  "license": "MIT",
  "description": "Babylon BSN Registry - A registry of Bitcoin Staking Node (BSN) information for testnet and mainnet",
  "devDependencies": {
    "prettier": "^3.6.2",
    "typescript": "^5.8.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
